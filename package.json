    "migrate:remote": "bunx wrangler d1 migrations apply sadakabox --remote"
  },
  "dependencies": {
    "chanfana": "^2.6.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.6.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.23.0",
    "@types/node": "22.13.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/service-worker-mock": "^2.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "wrangler": "^4.62.0"
  }
