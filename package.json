{
  "name": "cloudflare-workers-openapi",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite dev",
    "preview": "bun run build && bunx wrangler dev",
    "build": "bunx vite build",
    "deploy": "bun run build && bunx wrangler deploy",
    "cf-typegen": "bunx wrangler types",
    "db:generate": "bunx drizzle-kit generate",
    "migrate:local": "bunx wrangler d1 migrations apply sadaqahbox --local",
    "migrate:remote": "bunx wrangler d1 migrations apply sadaqahbox --remote",
    "auth:generate": "bunx @better-auth/cli generate --config src/auth/index.ts --output src/db/auth.schema.ts -y",
    "auth:format": "bunx prettier --write src/db/auth.schema.ts",
    "auth:update": "npm run auth:generate && npm run auth:format",
    "studio:dev": "drizzle-kit studio",
    "studio:prd": "cross-env NODE_ENV=production drizzle-kit studio"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@better-auth/passkey": "^1.4.18",
    "@daveyplate/better-auth-ui": "^3.3.15",
    "@fontsource-variable/geist": "^5.2.8",
    "@hono/zod-openapi": "^1.2.1",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.5",
    "@scalar/hono-api-reference": "^0.9.40",
    "@scalar/openapi-to-markdown": "^0.3.40",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "better-auth": "^1.4.18",
    "better-auth-cloudflare": "^0.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.8",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.13.0",
    "shadcn": "^3.8.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@cloudflare/vite-plugin": "^1.23.0",
    "@cloudflare/workers-types": "^4.20260207.0",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/service-worker-mock": "^2.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "wrangler": "^4.62.0"
  }
}
