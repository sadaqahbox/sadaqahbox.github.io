{
  "name": "cloudflare-workers-openapi",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run build && bunx wrangler dev",
    "dev:watch": "bunx concurrently \"bun run build:watch\" \"bunx wrangler dev\"",
    "build": "bunx vite build",
    "build:watch": "bunx vite build --watch",
    "deploy": "bun run build && bunx wrangler deploy",
    "cf-typegen": "bunx wrangler types",
    "generate": "bunx prisma generate",
    "migrate:local": "bunx wrangler d1 migrations apply sadakabox --local",
    "migrate:remote": "bunx wrangler d1 migrations apply sadakabox --remote"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "chanfana": "^2.6.3",
    "hono": "^4.6.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "22.13.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/service-worker-mock": "^2.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "prisma": "^7.3.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "wrangler": "^4.62.0"
  }
}
